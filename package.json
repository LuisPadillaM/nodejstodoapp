{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Useful boilerplate for web development",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000 --subdomain test",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "pre-start-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "keywords": [
    "es6",
    "eslint",
    "webpack",
    "react",
    "redux",
    "hot",
    "hmr",
    "reload",
    "npm scripts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LuisPadillaM/websetup_npmscripts"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "Luis Padilla",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^20.0.4",
    "jsdom": "^10.1.0",
    "json-schema-faker": "^0.5.0-rc5",
    "json-server": "^0.10.1",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "prop-types": "^15.5.10",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-toolbox": "^2.0.0-beta.12",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "ejs": "^2.5.6",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.6",
    "flux": "^3.1.2",
    "graphql": "^0.10.1",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.10.5",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.2",
    "postcss-cssnext": "^2.11.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.3"
  }
}
